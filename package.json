    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "sass": "^1.93.2",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  }
